{
  "name": "ola-mundo",
  "version": "1.0.0",
  "description": "Ola Mundo em GraphQl",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon src/server.ts --exec ts-node src/server.ts -e ts,graphql"
  },
  "keywords": [
    "graphql",
    "graphql-curso",
    "ola-mundo"
  ],
  "author": "Daniel Barbosa <daniel2005silva@gmail.com> (http://daniel2005silva.herokuapp.com)",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.25.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-tools": "^7.0.5",
    "http": "0.0.1-security",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^15.12.2"
  }
}
